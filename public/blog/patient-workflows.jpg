<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#f0f4f8"/>
  
  <!-- Background Pattern -->
  <rect x="0" y="0" width="800" height="600" fill="#4caf50" opacity="0.05"/>
  <circle cx="100" cy="500" r="150" fill="#4caf50" opacity="0.1"/>
  <circle cx="700" cy="100" r="100" fill="#4caf50" opacity="0.1"/>
  
  <!-- Workflow Diagram -->
  <!-- Node 1: Patient Arrival -->
  <circle cx="150" cy="200" r="50" fill="#4caf50" opacity="0.7"/>
  <text x="150" y="200" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Patient</text>
  <text x="150" y="220" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Arrival</text>
  
  <!-- Arrow 1 -->
  <path d="M200,200 L300,200" stroke="#4caf50" stroke-width="3" fill="none" marker-end="url(#arrow)"/>
  
  <!-- Node 2: Check-in -->
  <circle cx="350" cy="200" r="50" fill="#4caf50" opacity="0.7"/>
  <text x="350" y="200" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Check-in</text>
  <text x="350" y="220" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Process</text>
  
  <!-- Arrow 2 -->
  <path d="M400,200 L500,200" stroke="#4caf50" stroke-width="3" fill="none" marker-end="url(#arrow)"/>
  
  <!-- Node 3: Consultation -->
  <circle cx="550" cy="200" r="50" fill="#4caf50" opacity="0.7"/>
  <text x="550" y="200" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Doctor</text>
  <text x="550" y="220" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Consultation</text>
  
  <!-- Arrow 3 -->
  <path d="M550,250 L550,300" stroke="#4caf50" stroke-width="3" fill="none" marker-end="url(#arrow)"/>
  
  <!-- Node 4: AI Documentation -->
  <circle cx="550" cy="350" r="50" fill="#4caf50" opacity="0.7"/>
  <text x="550" y="350" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">AI Speech</text>
  <text x="550" y="370" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Recording</text>
  
  <!-- Arrow 4 -->
  <path d="M500,350 L400,350" stroke="#4caf50" stroke-width="3" fill="none" marker-end="url(#arrow)"/>
  
  <!-- Node 5: Processing -->
  <circle cx="350" cy="350" r="50" fill="#4caf50" opacity="0.7"/>
  <text x="350" y="350" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Processing</text>
  
  <!-- Arrow 5 -->
  <path d="M300,350 L200,350" stroke="#4caf50" stroke-width="3" fill="none" marker-end="url(#arrow)"/>
  
  <!-- Node 6: Documentation -->
  <circle cx="150" cy="350" r="50" fill="#4caf50" opacity="0.7"/>
  <text x="150" y="350" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Medical</text>
  <text x="150" y="370" font-family="Arial" font-size="14" fill="white" text-anchor="middle" font-weight="bold">Records</text>
  
  <!-- Document Icons -->
  <rect x="100" y="450" width="600" height="10" rx="5" fill="#4caf50" opacity="0.3"/>
  <rect x="150" y="470" width="500" height="10" rx="5" fill="#4caf50" opacity="0.3"/>
  <rect x="200" y="490" width="400" height="10" rx="5" fill="#4caf50" opacity="0.3"/>
  <rect x="250" y="510" width="300" height="10" rx="5" fill="#4caf50" opacity="0.3"/>
  
  <!-- Arrow markers definition -->
  <defs>
    <marker id="arrow" viewBox="0 0 10 10" refX="10" refY="5" 
            markerWidth="6" markerHeight="6" orient="auto">
      <path d="M 0 0 L 10 5 L 0 10 z" fill="#4caf50"/>
    </marker>
  </defs>
  
  <!-- Title "Patient Workflows" -->
  <text x="400" y="560" font-family="Arial" font-size="24" fill="#4caf50" text-anchor="middle" font-weight="bold">Patient Workflows</text>
</svg> 